<div class="row">
  {{#each model.ideas as |idea|}}
  <div class="col s12">
    <div class="card">
      <div class="card-content">
        <span class="card-title grey-text text-darken-4">{{idea.name}}</span>
        <p>{{idea.description}}</p>
      </div>
      <div class="card-action">
        {{#link-to 'projects.show' project}}Show{{/link-to}}
        {{#link-to 'projects.edit' project}}Edit{{/link-to}}
      </div>
    </div>
  </div>
  {{/each}}
</div>
